{
  "name": "photo-gallery",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=development tsc webpack.config.ts && NODE_ENV=development webpack --config ./webpack.config.js",
    "watch": "NODE_ENV=development tsc webpack.config.ts && NODE_ENV=development webpack --config ./webpack.config.js --progress --watch",
    "dev-server": "tsc webpack.config.ts && NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "test": "jest --config jest.config.json --watch",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "build-client-production": "rimraf ./build/client && NODE_ENV=production tsc webpack.config.ts && NODE_ENV=production webpack --config ./webpack.config.js",
    "build-server-production": "rimraf ./build/server && NODE_ENV=production tsc -p ./tsconfig.server.json",
    "build-production": "npm run build-client-production && npm run build-server-production",
    "start-production-server": "npm run build-production && pm2 start build/server/server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@types/html-webpack-plugin": "^2.11.2",
    "@types/image-size": "0.0.29",
    "@types/jest": "^19.2.3",
    "@types/react-hot-loader": "^3.0.1",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/webpack": "^2.2.11",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-dev-server": "^1.12.6",
    "image-size": "^0.6.2",
    "is-image": "^2.0.0",
    "jest": "^19.0.2",
    "pm2": "^2.10.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.2",
    "ts-loader": "^2.0.2",
    "ts-node": "5.0.1",
    "typescript": "2.7.2",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.18",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "glamor": "^3.0.0-1",
    "glamor-reset": "^3.0.0-1",
    "glamor-server": "^3.0.0-1",
    "sitemap": "^1.12.0",
    "sitemap-generator": "^6.0.0"
  }
}
